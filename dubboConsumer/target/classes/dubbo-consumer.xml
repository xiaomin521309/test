<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-consumer" />
    <!--<dubbo:registry  address="multicast://172.28.22.74:1234"172.28.3.21:2181 />-->
    <dubbo:registry address="zookeeper://172.28.22.74:2181"/>
    <!--  <dubbo:registry address="${registry-address}"/>-->

    <dubbo:reference id="hService" interface="main.java.helloService" />
    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>

</beans>