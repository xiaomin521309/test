<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="dubbo-provider" />
    <dubbo:registry address="zookeeper://172.28.22.74:2181" protocol="zookeeper"/>
    <!-- <dubbo:registry address="zookeeper://192.168.0.100:2181" protocol="zookeeper"/>-->
     <dubbo:protocol name="dubbo" port="20880"  />


     <bean id="hService" class="main.java.service.impl.helloServiceImpl" />
     <dubbo:service interface="main.java.helloService" ref="hService"  timeout="10000"/>


 </beans>